<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mmm.mytestutil">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/my_util"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/my_util"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".music_play.MusicActivity"/>
        <activity android:name=".popupwindow.PopUpActivity" />
        <activity android:name=".eventbus.EventBusDemoActivity" />
        <activity android:name=".rv_rv_nested.RvInRvActivity" />
        <activity android:name=".drawerLayout.DrawerActivity" />
        <activity android:name=".two_rv_link.TwoRecyclerActivity" />
        <activity android:name=".rv_vp_rv_nested.NestedRecyclerActivity" />
        <activity android:name=".glide.GlideActivity" />
        <activity android:name=".viewpager2.Vp2TestActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>